{
  "name": "dcandido/code-sniffer",
  "description": "CodeSniffer.",
  "type": "library",
  "license": "MIT",
  "require": {
    "squizlabs/php_codesniffer": "^3.6"
  },
  "extra": {
    "installer-paths": {
      ".": ["vendor/code-sniffer/phpcs.xml"]
    }
  },
  "scripts": {
    "validate-composer": "@composer validate --no-check-all --strict"
  },
  "config": {
    "sort-packages": true
  }
}